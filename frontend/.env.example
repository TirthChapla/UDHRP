# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000

# Authentication
VITE_AUTH_TOKEN_KEY=doctor_ai_auth_token
VITE_REFRESH_TOKEN_KEY=doctor_ai_refresh_token

# Azure/Cloud Services (if applicable)
VITE_AZURE_STORAGE_URL=
VITE_AZURE_STORAGE_CONTAINER=

# Payment Gateway (if applicable)
VITE_PAYMENT_KEY=
VITE_PAYMENT_SECRET=

# Google Maps API (for doctor location)
VITE_GOOGLE_MAPS_API_KEY=

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=.pdf,.jpg,.jpeg,.png,.doc,.docx

# Environment
VITE_ENV=development
VITE_DEBUG=true

# Feature Flags
VITE_ENABLE_BOOKING=true
VITE_ENABLE_PAYMENTS=true
VITE_ENABLE_VIDEO_CONSULTATION=true
VITE_ENABLE_LAB_REPORTS=true
